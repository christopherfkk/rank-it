{
  "expo": {
    "name": "rank-it",
    "slug": "rank-it",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/rank-it-logo.png",
    "userInterfaceStyle": "light",
    "scheme": "myapp",
    "splash": {
      "image": "./assets/rank-it-logo.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "plugins": [
      [
        "expo-image-picker",
        {
          "photosPermission": "The app accesses your photos to let you share them with your friends."
        }
      ]
    ],
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.rankit.rankit"
    },
    "android": {
      "package": "com.rankit.rankit",
      "adaptiveIcon": {
        "foregroundImage": "./assets/rank-it-logo.png",
        "backgroundColor": "#FFFFFF"
      }
    },
    "web": {
      "favicon": "./assets/rank-it-logo.png",
      "bundler": "metro"
    }
  }
}
